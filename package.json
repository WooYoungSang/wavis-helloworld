{
  "name": "demeter-wavis-framework",
  "version": "1.0.0",
  "description": "Complete AI-driven development framework with SSOT-based requirements management and contract engineering",
  "main": "demeter/ai/development-cycle.py",
  "keywords": [
    "ai-driven-development",
    "ssot",
    "contract-engineering",
    "claude-code",
    "graphrag",
    "development-framework",
    "requirements-management",
    "automated-testing",
    "quality-assurance",
    "project-template"
  ],
  "homepage": "https://github.com/wavis-alchemy/demeter",
  "repository": {
    "type": "git",
    "url": "https://github.com/wavis-alchemy/demeter.git"
  },
  "bugs": {
    "url": "https://github.com/wavis-alchemy/demeter/issues"
  },
  "license": "MIT",
  "author": {
    "name": "WAVIS Alchemy",
    "email": "info@wavis-alchemy.com",
    "url": "https://wavis-alchemy.com"
  },
  "contributors": [
    {
      "name": "Demeter Framework Team",
      "email": "demeter@wavis-alchemy.com"
    }
  ],
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.9.0"
  },
  "scripts": {
    "quick-start": "./quick-start.sh",
    "setup": "./quick-start.sh",
    "copy-template": "./demeter/scripts/copy-template.sh",
    "dev-cycle": "./demeter/ai/automate-dev-cycle.sh",
    "verify": "./scripts/quick-sync-check.sh",
    "full-verify": "./scripts/full-verification.sh",
    "test": "python3 -m pytest tests/",
    "test-coverage": "python3 -m pytest --cov=. --cov-report=html tests/",
    "ssot-verify": "python3 demeter/core/ssot/verification/verify-ssot.py",
    "contracts-verify": "python3 demeter/core/ssot/contracts/verify-contracts.py --all",
    "graphrag-sync": "python3 demeter/core/ssot/graphrag/sync/sync-engine.py",
    "dashboard": "python3 demeter/dashboard/app.py",
    "health-check": "python3 demeter/health/health-check.py",
    "migration": "python3 demeter/migration/migrate.py"
  },
  "bin": {
    "demeter": "./demeter/cli/demeter-cli.sh",
    "demeter-quick-start": "./quick-start.sh",
    "demeter-copy": "./demeter/scripts/copy-template.sh",
    "demeter-dev-cycle": "./demeter/ai/automate-dev-cycle.sh"
  },
  "directories": {
    "bin": "./demeter/cli",
    "doc": "./docs",
    "example": "./demeter/examples",
    "test": "./tests"
  },
  "files": [
    "demeter/",
    "docs/",
    "scripts/",
    "quick-start.sh",
    "VERSION",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ],
  "config": {
    "framework_version": "1.0.0",
    "default_tech_stack": "agnostic",
    "ssot_path": "demeter/core/ssot/framework-requirements.yaml",
    "contracts_path": "demeter/core/ssot/contracts/",
    "graphrag_path": "demeter/core/ssot/graphrag/"
  },
  "dependencies": {
    "pyyaml": "^6.0.0",
    "jinja2": "^3.1.0",
    "click": "^8.1.0",
    "rich": "^13.7.0",
    "fastapi": "^0.104.0",
    "uvicorn": "^0.24.0"
  },
  "devDependencies": {
    "pytest": "^7.4.0",
    "pytest-cov": "^4.1.0",
    "black": "^23.9.0",
    "flake8": "^6.1.0",
    "mypy": "^1.6.0"
  },
  "optionalDependencies": {
    "graphrag": "^0.1.0",
    "openai": "^1.3.0",
    "anthropic": "^0.7.0"
  },
  "peerDependencies": {
    "docker": ">=20.0.0",
    "git": ">=2.30.0"
  },
  "bundledDependencies": [],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/wavis-alchemy"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "preferGlobal": false,
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "workspaces": [
    "demeter/*",
    "examples/*"
  ],
  "demeter": {
    "framework": {
      "version": "1.0.0",
      "type": "universal",
      "philosophy": "From Requirements to Reality, Guided by AI"
    },
    "features": {
      "ai_integration": true,
      "claude_code": true,
      "ssot_driven": true,
      "contract_engineering": true,
      "graphrag": true,
      "multi_tech_stack": true,
      "quality_gates": true
    },
    "tech_stacks": [
      "python-fastapi",
      "typescript-nextjs",
      "go-gin",
      "java-spring"
    ],
    "extensions": {
      "domain": [
        "e-commerce",
        "fintech",
        "healthcare"
      ],
      "features": [
        "ai-ml",
        "blockchain",
        "iot",
        "streaming"
      ],
      "compliance": [
        "gdpr",
        "hipaa",
        "pci-dss"
      ]
    }
  }
}